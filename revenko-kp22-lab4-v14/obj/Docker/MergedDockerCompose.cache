name: dockercompose1586312482777431315
services:
  api:
    build:
      context: D:\school\RabbitMQ\revenko-kp22-lab4-v14
      dockerfile: API/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: api
    networks:
      default: null
    ports:
      - mode: ingress
